                <div class="container mt-50">
                    <h2 style="text-align: center;">Top Selling Products</h2>
                    <div class="table-responsive">
                        <table class="table table-bordered table-hover">
                            <thead style="background-color: #088178; color: white;">
                                <tr>
                                    <th>Product ID</th>
                                    <th>Product Name</th>
                                    <th>Image</th>
                                    <th>Price</th>
                                    <th>Selling Price</th>
                                    <th>Total Sales</th>
                                    <th>Category</th>
                                </tr>
                            </thead>
                            <tbody>
                                <!-- Loop through productDetails array and generate table rows -->
                                <% if (productData.length>0) { %>
        
                                    <% productData.forEach(product=> { %>
                                        <tr>
                                            <td>
                                                <%= product._id%>
                                            </td>
                                            <td>
                                                <%= product.producttitle%>
                                            </td>
                                            <td><img src="/uploads/<%= product.images[0] %>" class="img-thumbnail"
                                                    alt="Product Image" style="max-width: 70px; height: 70px;"></td>
                                            <td>
                                                <%= product.price %>
                                            </td>
                                            <td>
                                                <%= product.sellingPrice %>
                                            </td>
                                            <td>
                                                <%= orderData.items[0].quantity %>
                                            </td>
                                            <td>
                                                <%= product.category %>
                                            </td>
                                        </tr>
                                        <% }); %>
                                            <% } else { %>
                                                <TD>No Data available</TD>
                                                <% } %>
        
                            </tbody>
                        </table>
                    </div>
                </div>


