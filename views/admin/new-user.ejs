  <script>


        function validateForm() {
            // Trimming spaces
            var nameInput = document.forms["editUserForm"]["name"];
            nameInput.value = nameInput.value.trim();

            var emailInput = document.forms["editUserForm"]["email"];
            emailInput.value = emailInput.value.trim();

            var mobileInput = document.forms["editUserForm"]["mobile"];
            mobileInput.value = mobileInput.value.trim();

            // Check if any field is just spaces
            if (nameInput.value === "" || emailInput.value === "" || mobileInput.value === "") {
                alert("Please fill out all fields.");
                return false;
            }

            return true;
        }
  </script>
