    <main class="main">
        <div class="page-header breadcrumb-wrap">
            <div class="container">
                <div class="breadcrumb">
                    <a href="index.html" rel="nofollow">Home</a>
                    <span></span> Shop
                    <span></span> Your Cart
                </div>
            </div>
        </div>
        <section class="mt-50 mb-50">
            <div class="container">
                <div class="row">
                    <div class="col-12">

                        <% if (productData && productData.length> 0) { %>

                            <div class="table-responsive">

                                <table class="table shopping-summery text-center clean">
                                    <thead>
                                        <tr class="main-heading">
                                            <th scope="col">Image</th>
                                            <th scope="col">Name</th>
                                            <th scope="col">Price</th>
                                            <th scope="col">Quantity</th>
                                            <th scope="col">Size</th>
                                            <th scope="col">Subtotal</th>
                                            <th scope="col">Remove</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% for (let i=0; i < productData.length; i++) { %>


                                            <tr>
                                                <td class="image product-thumbnail">
                                                    <img src="/uploads/<%=productData[i].images[1] %>" alt="#">
                                                </td>
                                                <td class="productname">
                                                    <br>
                                                    <h5 class="productname"><a
                                                            href="/productDetails?id=<%= productData[i]._id  %>">
                                                            <%= productData[i].producttitle %>
                                                        </a></h5>
                                                    <p class="font-xs">
                                                        <%=productData[i].categoryId %><br>
                                                    </p>
                                                </td>
                                                <td class="price" id="Price<%=i%>" data-title="Price"><span>
                                                        <%=productData[i].price %>
                                                    </span></td>
                                                <td class="text-center" data-title="Stock">

                                                    <button class="btn btn-sm increment-button" style="width: 1px;"
                                                        onclick="changingQuantity('<%= userCart.items[i].subTotal %>','<%= userCart.items[i].quantity %>','<%= userCart.items[i]._id %>', +1 ,'<%= userCart.owner %>','<%= productData[i]._id %>','<%= userCart.items[i].size %>','<%=i%>','<%= productData[i].price %>')">+</button>
                                                    <span class="qty-val" id="incrementQuantity<%=i%>">
                                                        <%= userCart.items[i].quantity %>
                                                    </span>
                                                    <button class="btn btn-sm increment-button" style="width: 1px;"
                                                        onclick="changingQuantity('<%= userCart.items[i].subTotal %>','<%= userCart.items[i].quantity %>','<%= userCart.items[i]._id %>', -1 , '<%= userCart.owner %>','<%= productData[i]._id %>','<%= userCart.items[i].size %>','<%=i%>','<%= productData[i].price %>')">-</button>

                                                </td>
                                                <td class="price" data-title="Size"><span>

                                                        <%=userCart.items[i].size %>
                                                    </span></td>
                                                <td class="text-right" id="Subtotal<%=i%>" data-title="Subtotal"><span>
                                                        <%= userCart.items[i].subTotal %>
                                                    </span></td>
                                                <td class="action" data-title="Remove">
                                                    <a href="#" onclick="removeItem('<%= productData[i]._id %>')"
                                                        class="text-muted"><i class="fi-rs-trash"></i></a>
                                                </td>
                                            </tr>
                                            <% } %>

                                    </tbody>
                                </table>

                            </div>
                            <% } else { %>
                                <tr>
                                    <td colspan="7" class="text-center" style="font-size: 18px; font-weight: bold;">
                                        There is no products in your cart. Please shop.
                                    </td>
                                    
                                </tr>
                                <% } %>

                                    <div class="cart-action text-end">
                                        <!-- <a href="#" class="btn mr-10 mb-sm-15"><i class="fi-rs-shuffle mr-10"></i>Update Cart</a> -->
                                        <a href="dashboard" class="btn"><i class="fi-rs-shopping-bag mr-10"></i>Continue
                                            Shopping</a>
                                    </div>
                                    <div class="divider center_icon mt-50 mb-50"><i class="fi-rs-fingerprint"></i></div>
                                    <div class="container">
                                        <div class="row">
                                            <div class="col-lg-6 col-md-12">
                                                <form>
