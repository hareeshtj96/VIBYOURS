                                        <script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>

                                        <script>
                                            async function removeItem(itemId) {
                                                try {

                                                    const confirmation = await Swal.fire({
                                                        title: 'Are you sure?',
                                                        text: 'You won\'t be able to revert this!',
                                                        icon: 'warning',
                                                        showCancelButton: true,
                                                        confirmButtonColor: '#3085d6',
                                                        cancelButtonColor: '#d33',
                                                        confirmButtonText: 'Yes, remove it!'
                                                    });


                                                    if (confirmation.isConfirmed) {
                                                        const response = await fetch(`/removeItem?itemId=${itemId}`, {
                                                            method: 'GET',
                                                        });

                                                        const result = await response.json();

                                                        if (result.status === true) {

                                                            Swal.fire({
                                                                title: 'Deleted!',
                                                                text: 'Your item has been deleted.',
                                                                icon: 'success'
                                                            });
                                                            // Reload the current page
                                                            window.location.reload();


                                                            console.log('Item removed successfully');


                                                        } else {

                                                            Swal.fire({
                                                                title: 'Error!',
                                                                text: 'Failed to remove item.',
                                                                icon: 'error'
                                                            });

                                                            console.log('Failed to remove item');
                                                        }
                                                    }
                                                } catch (error) {

                                                    Swal.fire({
                                                        title: 'Error!',
                                                        text: 'An error occurred while removing the item.',
                                                        icon: 'error'
                                                    });

                                                    console.error('Error removing item:', error);
                                                }

                                            }




                                            function changingQuantity(subtotal, currentQuantity, itemId, newQuantity, owner, productId, size, index, price) {


                                                const Quantity = document.getElementById(`incrementQuantity${index}`).innerText;
                                                const updatingQuantity = parseInt(Quantity) + parseInt(newQuantity);
                                                const updateQty = document.getElementById(`incrementQuantity${index}`);
                                                const subTotal = document.getElementById(`Subtotal${index}`);
                                                const total = document.getElementById('grandTotal');
                                                const itemsTotal = document.getElementById('total');

                                                fetch("/changingQuantity", {
                                                    method: "POST",
                                                    headers: {
                                                        "Content-Type": "application/json",
                                                    },
                                                    body: JSON.stringify({
                                                        subtotal: subtotal,
                                                        updatingQuantity: updatingQuantity,
                                                        itemId: itemId,
                                                        userId: owner,
                                                        productId: productId,
                                                        size: size,
                                                        index: index,
                                                    }),
                                                })
                                                    .then(response => {
                                                        if (!response.ok) {
                                                            throw new Error("Request failed");
                                                        }
                                                        return response.json();
                                                    })
                                                    .then(responseData => {
                                                        if (responseData.status === "out of stock") {
                                                            Swal.fire({
                                                                title: "<span style='color: red;'>Out Of Stock</span>",
                                                                html: "<span style='color: blue;'>Please keep in touch with us</span>",
                                                                icon: "error",
                                                                timer: 2000,
                                                                showConfirmButton: false,
                                                            });
                                                        } else if (responseData.status === "maximumQuantity") {
                                                            Swal.fire({
                                                                title: "<span style='color: red;'>Maximum Quantity Reached</span>",
                                                                html: "<span style='color: blue;'>You have reached the maximum allowed quantity</span>",
                                                                icon: "warning",
                                                                timer: 2000,
                                                                showConfirmButton: false,
                                                            });
                                                        } else if (responseData.status === "success") {
                                                            updateQty.innerHTML = updatingQuantity;
                                                            subTotal.innerHTML = updatingQuantity * price;
                                                            total.innerText = responseData.total;
                                                            itemsTotal.innerText = responseData.total;


                                                        }
                                                    })
                                                    .catch(error => {
                                                        console.error(error);
                                                    });
                                            }












                                        </script>

    </main>
